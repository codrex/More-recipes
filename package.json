{
  "name": "more-recipe",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "wp": "webpack-dev-server --hot --history-api-fallback --open",
    "test": "cross-env NODE_ENV=test nyc mocha server/test/test.js ",
    "test:db": "set NODE_ENV=test:s&&sequelize db:migrate:undo:all&&sequelize db:migrate  && mocha --compilers js:babel-core/register --timeout 10000 server/test/test.js",
    "s:test": "set NODE_ENV=test:s&& mocha --compilers js:babel-core/register --timeout 10000 server/test/test.js",
    "heroku-postbuild":"sequelize db:migrate",
    "migrate": "sequelize db:migrate:undo:all&&sequelize db:migrate",
    "start": "babel-node server/bin/www",
    "start:dev": "nodemon --exec babel-node server/bin/www",
    "coverage": "set NODE_ENV=test&&sequelize db:migrate:undo:all&&sequelize db:migrate&& cross-env NODE_ENV=test nyc mocha server/test/test.js",
    "shc": "start coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --compilers js:babel-core/register --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "Ogbemudia Rex",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.7.0",
    "nodemon": "^1.12.0",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "sequelize": "^4.8.0",
    "sequelize-cli": "^2.7.0",
    "validate": "^3.0.1",
    "validate.js": "^0.11.1",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.5.3",
    "nyc": "^11.1.0",
    "supertest": "^3.0.0"
  }
}
